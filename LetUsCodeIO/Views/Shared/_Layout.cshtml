<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="keywords" content="LetUsCode,Let Us Code,Code,E-Learning">
    <meta property="og:site_name" content="OurCode!" />
    <meta property="og:url" content="http://ourco.de/" />
    <meta property="og:type" content="website" />
    <meta property="og:description" content="Our.co is an interactive e-learning coding site. Students will be able to create new features of Let Us Code. We take different selected lesson plans outlines in the Create section, then will provide different and new features that everyone can use! Students, teachers and parents will be taught and asked to participate with the different parts of the application process." />
    <meta name="description" content="OurCode is an interactive e-learning coding site. Students will be able to create new features of Let Us Code. We take different selected lesson plans outlines in the Create section, then will provide different and new features that everyone can use! Students, teachers and parents will be taught and asked to participate with the different parts of the application process." />
    <title>Our Code!</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <link rel="icon" type="image/x-icon" href="~/Content/images/favicon2.png">
    <link rel="image_src" href="~/Content/images/logo-ourcode.png" />


    <script>
        (function (i, s, o, g, r, a, m) {
            i['GoogleAnalyticsObject'] = r; i[r] = i[r] || function () {
                (i[r].q = i[r].q || []).push(arguments)
            }, i[r].l = 1 * new Date(); a = s.createElement(o),
            m = s.getElementsByTagName(o)[0]; a.async = 1; a.src = g; m.parentNode.insertBefore(a, m)
        })(window, document, 'script', '//www.google-analytics.com/analytics.js', 'ga');

        ga('create', 'UA-74417204-1', 'auto');
        ga('send', 'pageview');

    </script>
</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <a id="small-logo-link" href="~/Home">
                    <img src="~/Content/images/our-code-small.png" alt="Let Us Code!" />  
                    <!--<div class="mainour-code-small"></div>-->
                </a>
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <!--a asp-controller="Home" asp-action="Index" class="navbar-brand"-->
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li>
                        <a id="main-logo-link" href="~/Home">
                            <!--<div class="mainlogo-ourcode"></div>-->
                            <img src="~/Content/images/logo-ourcode.png" alt="Let Us Code!" />
                        </a>
                    </li>
                    <!--<li><a asp-controller="Home" asp-action="About">Ideas</a></li>
        <li><a asp-controller="Home" asp-action="About">Design</a></li>
        <li><a asp-controller="Home" asp-action="Contact">About</a></li>
        <li><a asp-controller="Home" asp-action="Contact">Contact</a></li>-->


                </ul>
                @Html.Partial("_LoginPartial")
            </div>
        </div>
    </div>
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>
                &copy; - OurCo.de&trade;
            </p>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
